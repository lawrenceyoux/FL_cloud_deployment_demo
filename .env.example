# Copy this file to .env and fill in your values
# .env is gitignored â€” NEVER commit real credentials

# AWS
AWS_PROFILE=fl-demo
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=123456789012

# S3 Buckets
S3_BUCKET_HOSPITAL_1=fl-demo-data-hospital-1
S3_BUCKET_HOSPITAL_2=fl-demo-data-hospital-2
S3_BUCKET_HOSPITAL_3=fl-demo-data-hospital-3
S3_MODEL_BUCKET=fl-demo-models
S3_MLFLOW_BUCKET=fl-demo-mlflow

# MLflow
MLFLOW_TRACKING_URI=http://localhost:5000

# FL Server
FL_SERVER_ADDRESS=localhost:8080
FL_NUM_ROUNDS=50
FL_MIN_CLIENTS=3

# Differential Privacy
DP_EPSILON=1.0
DP_DELTA=1e-5
DP_MAX_GRAD_NORM=1.0
